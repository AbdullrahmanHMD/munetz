{
  "manifest_version": 3,
  "name": "MuNetz Chatbot",
  "version": "1.0",
  "description": "A React-based chatbot extension",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon32.png",
    "128": "images/icon192.png"
  }
}
